{
  "name": "re-fetch",
  "version": "1.0.0",
  "description": "Type-safe fetch wrapper with retries, timeouts, caching, and error handling. Works in browser, Node.js, and React Native.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "retry",
    "timeout",
    "cache",
    "typescript",
    "react-native"
  ],
  "author": "Sergei Golishnikov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeigolishnikov/re-fetch.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/whatwg-fetch": "^0.0.33",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.test.ts",
      "!src/__tests__/**"
    ]
  }
}
