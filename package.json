{
  "name": "re-fetch",
  "version": "1.0.0",
  "description": "Type-safe fetch wrapper with retries, timeouts, caching, and error handling. Works in browser, Node.js, and React Native.",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "example:server": "node examples/server.js",
    "example:client": "ts-node examples/client.ts",
    "example:public-api": "ts-node examples/public-api.ts"
  },
  "keywords": [
    "fetch",
    "http",
    "request",
    "retry",
    "timeout",
    "cache",
    "typescript",
    "react-native"
  ],
  "author": "Sergei Golishnikov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeigolishnikov/re-fetch.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.10.1",
    "@types/whatwg-fetch": "^0.0.33",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=16"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.test.ts",
      "!src/__tests__/**"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
